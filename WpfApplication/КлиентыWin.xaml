<Window x:Class="WpfApplication.КлиентыWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Постоянные клиенты" Height="350" Width="525">
    <Grid>
        <Button x:Name="back" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="37" Click="back_Click" Margin="480,0,0,0">
            <Image Source="1.png" Width="37" Height="30" />
        </Button>
        <TextBox x:Name="list_of_clients" HorizontalAlignment="Left" Height="226" Margin="17,56,0,0" TextWrapping="Wrap" Text="Список клиентов" VerticalAlignment="Top" Width="253"/>
        <Button x:Name="insert_in_check" Content="Добавить в чек" HorizontalAlignment="Left" Margin="411,289,0,0" VerticalAlignment="Top" Width="96" Click="button_Click"/>
        <TextBox x:Name="name_of_client" HorizontalAlignment="Left" Height="17" Margin="17,23,0,0" TextWrapping="Wrap" Text="Имя клиента" VerticalAlignment="Top" Width="167"/>
        <Button x:Name="search" Content="Поиск" HorizontalAlignment="Left" Height="17" Margin="199,23,0,0" VerticalAlignment="Top" Width="46"/>
    </Grid>
</Window>

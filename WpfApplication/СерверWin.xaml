<Window x:Class="WpfApplication.СерверWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Сервер" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="average_profit" HorizontalAlignment="Left" Height="46" Margin="28,44,0,0" TextWrapping="Wrap" Text="Средняя прибыль за месяц" VerticalAlignment="Top" Width="194"/>
        <TextBox x:Name="profit_for_fuel" HorizontalAlignment="Left" Height="40" Margin="28,106,0,0" TextWrapping="Wrap" Text="Прибыль за топливо" VerticalAlignment="Top" Width="194"/>
        <TextBox x:Name="profit_for_a_good_or_service" HorizontalAlignment="Left" Height="40" Margin="28,167,0,0" TextWrapping="Wrap" Text="Прибыль за товары и услуги" VerticalAlignment="Top" Width="194"/>
        <Button x:Name="сalculate" Content="Рассчитать" HorizontalAlignment="Left" Height="39" Margin="324,195,0,0" VerticalAlignment="Top" Width="117" Click="button_Click"/>
        <Calendar HorizontalAlignment="Left" Height="170" Margin="257,10,0,0" VerticalAlignment="Top" Width="184"/>


    </Grid>
</Window>
